{
    "name": "amazeelabs/algm_drutiny",
    "description": "Core repository that contains policies and profiles we use to run ALGM drutiny audits",
    "type": "library",
    "keywords": ["drutiny", "audit", "performance", "security", "accessibility"],
    "license": "MIT",
    "authors": [
        {
            "name": "Blaize Kaye",
            "email": "blaize.kaye@amazee.com"
        },
        {
            "name": "Tim Clifford",
            "email": "tim.clifford@amazee.com"
        },
        {
            "name": "Fran Garcia-Linares",
            "email": "fran.garcia@amazee.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "drutiny/drutiny": "^2.4",
        "phpunit/phpunit": "^5.0",
        "drutiny/plugin-drupal-8": "^2.0",
        "squizlabs/php_codesniffer": "2.*",
        "symfony/yaml": "^3.2 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Drutiny\\algm\\": "src/",
            "DrutinyTests\\algm\\": "tests/src/"
        }
    }
}
